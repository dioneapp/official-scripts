{
    "installation": [
        {
            "name": "Creating environment",
            "env": "env",
            "commands": [
                "uv pip install open-webui"
            ]
        }
    ],
    "start": [
        {
            "name": "Starting open-webui",
            "env": "env",
            "catch": "8080",
            "commands": [
               "uvx --python 3.11 open-webui@latest serve"
            ]
        }
    ]
}