{
    "installation": [
        {
            "name": "Creating environment",
            "env": {
                "name": "env",
                "type": "conda",
                "version": "3.11"
            },
            "commands": [
                "pip install open-webui"
            ]
        }
    ],
    "start": [
        {
            "name": "Starting open-webui",
            "env": {
                "name": "env",
                "type": "conda",
                "version": "3.11"
            },
            "catch": "8080",
            "commands": [
                "open-webui serve --port 8080 --host 127.0.0.1"
            ]
        }
    ]
}